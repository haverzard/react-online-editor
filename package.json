{
  "name": "react-online-editor",
  "version": "0.0.0",
  "description": "Write your React components in React!",
  "author": "haverzard",
  "sideEffects": false,
  "license": "GNU GPL-3.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "prepublishxx": "npm run clean && tsc && npm run transpile-lib && npm run transpile-esm",
    "clean": "rimraf esm && rimraf lib",
    "lint": "npx eslint **/**.js --fix && npx stylelint **/**.scss **/**.css --fix",
    "transpile-lib": "cross-env BABEL_ENV=cjs babel src --out-dir lib --extensions \".ts,.tsx\" --copy-files --source-maps true",
    "transpile-esm": "cross-env BABEL_ENV=esm babel src --out-dir esm --extensions \".ts,.tsx\" --copy-files",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "lib",
    "esm"
  ],
  "keywords": [
    "React",
    "codemirror",
    "editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haverzard/react-online-editor.git"
  },
  "bugs": {
    "url": "https://github.com/haverzard/react-online-editor/issues"
  },
  "homepage": "https://github.com/haverzard/react-online-editor",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@babel/standalone": "^7.14.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.1",
    "react-redux": "^7.2.4"
  },
  "peerDependencies": {
    "codemirror": "^5.62.2",
    "react": "^17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.4.0-alpha.33",
    "@types/babel__standalone": "^7.1.3",
    "@types/jest": "^26.0.0",
    "@types/react-dom": "^17.0.9",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.1.0",
    "codemirror": "^5.62.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.0.1",
    "react": "^17.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^4.2.4"
  }
}
